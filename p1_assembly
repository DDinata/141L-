//program 1 - input addr 8-9, output addr 10-11
// TODO: CORNER CHECKS (0, 1, mmm is 15)

// $r1|$r0 - div
// $r3|$r2 - num
// $r6 - result setter
// $r7 - index
// $r8 - left shift amount

acc 8
ld $r1 // div[15:8]
acc 9
ld $r0 // div[7:0]

// acc 128
// TEMP
acc 8
shl $ra
shl $ra
shl $ra
shl $ra
mov $r3 // num[15:8] = 10000000
acc 0
mov $r2 // num[7:0] = 00000000
